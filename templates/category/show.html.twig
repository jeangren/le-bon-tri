{% extends 'base.html.twig' %}

{% block Usages endblock %}

{% block body %}
    <h1>Usages</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ category.name }}</td>
            </tr>
            <tr>
                <th>Devenir</th>
                <td>{{ category.devenir }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('category_index') }}">Retour à la liste des usages</a>

    <a href="{{ path('category_edit', {'id': category.id}) }}">Mettre à jour</a>

    {{ include('category/_delete_form.html.twig') }}
{% endblock %}
